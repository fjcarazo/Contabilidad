{$DESCRIPTION 'dxPack2 for Delphi7'}
{$IMPLICITBUILD OFF}

requires
  rtl,
  vcl,
  designide,
  vclactnband,
  vclx;

contains
  dxWinXPBar in 'dxWinXPBar.pas',
  dxContainer in 'dxContainer.pas',
  dxCore in 'dxCore.pas',
  dxCoreUtils in 'dxCoreUtils.pas',
  dxPropEditors in 'dxPropEditors.pas',
  dxRegister in 'dxRegister.pas',
  dxButtons in 'dxButtons.pas',
  dxCheckCtrls in 'dxCheckCtrls.pas';

end.
